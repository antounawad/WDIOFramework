<UserControl x:Class="Eulg.Setup.Pages.InstDone"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="480"
             d:DesignHeight="300"
             mc:Ignorable="d">
    <StackPanel x:Name="AreaInstDone">
        <Label Content="xbAV-Berater wurde erfolgreich installiert." FontSize="13" />
        <CheckBox x:Name="ChkStartEulg"
                  Margin="20,10,20,0"
                  VerticalAlignment="Center"
                  Content="xbAV-Berater starten"
                  FontSize="13" />
    </StackPanel>
</UserControl>
